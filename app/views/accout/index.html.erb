<h2>All record </h2>


<table>
  <tr>
    <th>Book Name</th>
    <th>Author email</th>
  </tr>

  <tbody>

    <% @account.each do |f| %>
      <tr>
        <td> <%= Book_bookName.find_by(f.book_id) %> </td>
        <td> <%= Author_email.find_by(f.author_id) %> </td>
        <!-- <td> <%= time_ago_in_words(f.created_at) %> ago </td> -->
      </tr>
    <% end %>
  <tbody>
</table>
